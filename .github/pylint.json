{
	"problemMatcher": [
		{
			"owner": "pylint_error",
			"severity": "error",
			"pattern": [
				{
					"regexp": "^(.*):(.*):(\\d+):\\s+\\[((E)\\d+)\\((.*)\\), (.*)\\]\\s+(.*)$",
					"file": 1,
					"line": 3,
					"code": 4,
					"message": 8
				}
			]
		},
		{
			"owner": "pylint_warning",
			"severity": "warning",
			"pattern" : [
				{
					"regexp": "^(.*):(.*):(\\d+):\\s+\\[((R|W)\\d+)\\((.*)\\), (.*)\\]\\s+(.*)$",
					"file": 1,
					"line": 3,
					"code": 4,
					"message": 8
				}
			]
		}
	]
}
